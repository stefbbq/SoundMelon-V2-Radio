:plain
	$('.callout#show-artist-profile').empty().html('#{j( render "artists/show_artist_profile", artist_id: @artist_id )}');
	centerRadioCallout('expand');
	TweenLite.to($('.radio-wrapper'), 0.5, {left: 0, ease: Sine.easeInOut})
	TweenLite.to($('.callout#show-artist-profile').closest('.callout-wrapper'), 0.5, {ease: Sine.easeInOut, css:{right: 8}, onComplete: function() {
	 	$('.callout#show-artist-profile').addClass('visible').closest('.callout-wrapper').css('z-index', '0');
	}});
	